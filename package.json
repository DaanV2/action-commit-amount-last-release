{
  "name": "action-commit-amount-last-release",
  "version": "1.0.17",
  "description": "This action returns the amount of commits made since the last release",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build ./out/source/action.js -m",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "preversion": "",
    "postversion": "git push && git push --tags",
    "version": "git add -A source",
    "test": "mocha --debug-brk",
    "clean": "rimraf lib",
    "pretest": "npm run build"
  },
  "keywords": [
    "github",
    "action",
    "changes",
    "utillituy"
  ],
  "author": "DaanV2",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@vercel/ncc": "^0.38.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.22",
    "chai": "^5.1.0",
    "mocha": "^10.3.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
